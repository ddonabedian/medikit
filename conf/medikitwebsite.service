[Unit]
Description=Medikit website
After=network.target

[Service]
ExecStart=/usr/bin/dotnet /home/ftpuser/src/build/results/services/Medikit.Website/Medikit.Website.dll --server.urls="http://*:7001"
KillMode=process
TimeoutStopSec=5s
Restart=always
WorkingDirectory=/home/ftpuser/src/build/results/services/Medikit.Website
Environment=ASPNETCORE_ENVIRONMENT=Production

[Install]
WantedBy=multi-user.target